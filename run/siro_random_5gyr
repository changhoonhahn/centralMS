# !/bin/bash
# PBS -l nodes=2:ppn=24
# PBS -N randomSFH_5gyr_abc 
cd $PBS_O_WORKDIR
export NPROCS=`wc -l $PBS_NODEFILE |gawk '//{print $1}'`
export PATH="/home/users/hahn/anaconda2/bin:$PATH"
/usr/local/openmpi-1.10.7/bin/mpiexec -np $NPROCS -npernode 24 python /home/users/hahn/projects/centralMS/centralms/run_abcee.py randomSFH_5gyr 15 1000 1 0 
